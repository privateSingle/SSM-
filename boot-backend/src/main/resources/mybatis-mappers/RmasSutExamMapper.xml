<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zw.admin.server.dao.RmasSutExamDao">

	<sql id="where">
		<where>
			<if test="params.id != null and params.id != ''">
				and id = #{params.id} 
			</if>
			<if test="params.examId != null and params.examId != ''">
				and examId = #{params.examId} 
			</if>
			<if test="params.examName != null and params.examName != ''">
				and examName = #{params.examName} 
			</if>
			<if test="params.examDate != null and params.examDate != ''">
				and examDate = #{params.examDate} 
			</if>
			<if test="params.grade != null and params.grade != ''">
				and grade = #{params.grade} 
			</if>
			<if test="params.term != null and params.term != ''">
				and term = #{params.term} 
			</if>
			<if test="params.stuId != null and params.stuId != ''">
			and stuId = #{params.stuId}
			</if>
			<if test="params.stuIdList != null and params.stuIdList.size > 0">
				and stuId in
				<foreach collection="params.stuIdList" item="id" index="index" open="(" close=")" separator=",">
					#{id}
				</foreach>
			</if>
			<if test="params.stuName != null and params.stuName != ''">
				and stuName = #{params.stuName}
			</if>
			<if test="params.yuwen != null and params.yuwen != ''">
				and yuwen = #{params.yuwen}
			</if>
			<if test="params.yuwenJc != null and params.yuwenJc != ''">
				and yuwenJc = #{params.yuwenJc}
			</if>
			<if test="params.yuwenyd != null and params.yuwenyd != ''">
				and yuwenyd = #{params.yuwenyd}
			</if>
			<if test="params.yuwenZw != null and params.yuwenZw != ''">
				and yuwenZw = #{params.yuwenZw}
			</if>
			<if test="params.shuxue != null and params.shuxue != ''">
				and shuxue = #{params.shuxue}
			</if>
			<if test="params.shuxueXz != null and params.shuxueXz != ''">
				and shuxueXz = #{params.shuxueXz}
			</if>
			<if test="params.shuxueJs != null and params.shuxueJs != ''">
				and shuxueJs = #{params.shuxueJs}
			</if>
			<if test="params.english != null and params.english != ''">
				and english = #{params.english}
			</if>
			<if test="params.sipin != null and params.sipin != ''">
				and sipin = #{params.sipin}
			</if>
			<if test="params.zf != null and params.zf != ''">
				and zf = #{params.zf}
			</if>
			<if test="params.context != null and params.context != ''">
				and context = #{params.context}
			</if>
			<if test="params.createTime != null and params.createTime != ''">
				and createTime = #{params.createTime}
			</if>
			<if test="params.updateTime != null and params.updateTime != ''">
				and updateTime = #{params.updateTime}
			</if>

		</where>
	</sql>

	<select id="count" resultType="int">
		select count(1) from rmas_sut_exam t
		<include refid="where" />
	</select>

	<select id="list" resultType="RmasSutExam">
		select * from rmas_sut_exam t
		<include refid="where" />
		${params.orderBy}
		limit #{offset}, #{limit}
	</select>

	<update id="update">
		update rmas_sut_exam t
		<set>
			<if test="examId != null">
				examId = #{examId},
			</if>
			<if test="examName != null">
				examName = #{examName},
			</if>
			<if test="examDate != null">
				examDate = #{examDate},
			</if>
			<if test="grade != null">
				grade = #{grade},
			</if>
			<if test="term != null">
				term = #{term},
			</if>
			<if test="stuId != null">
				stuId = #{stuId},
			</if>
			<if test="stuName != null">
				stuName = #{stuName},
			</if>
			<if test="yuwen != null">
				yuwen = #{yuwen},
			</if>
			<if test="yuwenJc != null">
				yuwenJc = #{yuwenJc},
			</if>
			<if test="yuwenyd != null">
				yuwenyd = #{yuwenyd},
			</if>
			<if test="yuwenZw != null">
				yuwenZw = #{yuwenZw},
			</if>
			<if test="shuxue != null">
				shuxue = #{shuxue},
			</if>
			<if test="shuxueXz != null">
				shuxueXz = #{shuxueXz},
			</if>
			<if test="shuxueJs != null">
				shuxueJs = #{shuxueJs},
			</if>
			<if test="english != null">
				english = #{english},
			</if>
			<if test="sipin != null">
				sipin = #{sipin},
			</if>
			<if test="zf != null">
				zf = #{zf},
			</if>
			<if test="context != null">
				context = #{context},
			</if>
			<if test="createTime != null">
				createTime = #{createTime},
			</if>
			<if test="updateTime != null">
				updateTime = #{updateTime},
			</if>

		</set>

		where t.id = #{id}
	</update>

</mapper>
